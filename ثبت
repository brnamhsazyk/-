    <script>
        window.onload = function() {
            startTime();
            setThumbHeight();
 
            const video = document.getElementById('video');
            video.play()
                .catch((err) => {
                    if (err.name === 'NotAllowedError') {
                        // Display PLAY button with a click event listener and play the video there.
                    }
                });
        };
 
        window.onresize = function() {
            setThumbHeight();
        }
 
        function startTime() {
            var today = new Date();
            var h = today.getHours();
            var m = today.getMinutes();
 
            m = checkTime(m);
            document.getElementById('phone-clock').innerHTML = h + ':' + m;
            var t = setTimeout(startTime, 1000);
        }
 
        function checkTime(i) {
            if (i < 10) {
                i = "0" + i
            };
            return i;
        }
 
        function setThumbHeight() {
            if (document.getElementById('card-posts').children.length > 0) {
                var width = document.getElementById('card-posts').children[0].offsetWidth;
                document.getElementById('card-posts').style.maxHeight = width + 'px';
            }
        }
    </script>
